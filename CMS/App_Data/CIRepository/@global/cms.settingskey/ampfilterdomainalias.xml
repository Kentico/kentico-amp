  <KeyDisplayName>Domain alias</KeyDisplayName>
  <KeyExplanationText />
  <KeyGUID>526cbce6-7fa9-4321-bf0d-0cfd1bb0ffd3</KeyGUID>
  <KeyIsGlobal>False</KeyIsGlobal>
  <KeyIsHidden>False</KeyIsHidden>
  <KeyName>AMPFilterDomainAlias</KeyName>
  <KeyOrder>2</KeyOrder>
  <KeyType>string</KeyType>
  <KeyValidation>
    <![CDATA[^((http|https)\:\/\/)?((([_a-zA-Z0-9][_a-zA-Z0-9\-]*\.)*[a-zA-Z0-9][a-zA-Z0-9\-]+)|((\d{1,3}\.){3}\d{1,3}))(\:\d{1,5})?(\/(\%[a-zA-Z0-9]{2}|[a-zA-Z0-9_()])*\/?)*$]]>
  </KeyValidation>
  <KeyValue>amp.domain.com</KeyValue>
</cms.settingskey>
